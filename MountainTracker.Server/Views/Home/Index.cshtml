@using MountainTracker.Server.Config;
@{
    Layout = null;
    string config = (string)ViewBag.clientConfig;
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>MountainTracker!</title>
    <base href="/">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="styles.css">
    <link rel="modulepreload" href="chunk-4KSUAUMS.js">
</head>
<body class="mat-typography">
    <script>
        //load config and remove self to hide
        loadConfig = function() {
            let config = '@Html.Raw(config)'
            globalThis.window.clientConfig = JSON.parse(config);
            currentScript = document.currentScript || document.scripts[0];
            currentScript.parentNode.removeChild(currentScript);
        } 
        loadConfig();
    </script>
    <app-root></app-root>
    <script src="polyfills.js" type="module"></script>
    <script src="main.js" type="module"></script>
</body>
</html>